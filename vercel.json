
{
    "version": 2,
    "builds": [
      {
        "src": "countdown/wsgi.py",  
        "use": "@vercel/python",
        "config":{"maxLambdaSize":"15mb","runtime":"python3.11.2",
        "includeFiles": ["requirements.txt", "vercel-build.bat"]
    }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "countdown/wsgi.py",
        "continue": true
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "countdown.settings" 
    }
  }
  
